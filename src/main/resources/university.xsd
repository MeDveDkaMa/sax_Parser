  </xs:simpleType>

  <xs:simpleType name="FirstNameStudent">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Alexander"/>
      <xs:enumeration value="Bogdan"/>
      <xs:enumeration value="Vasily"/>
